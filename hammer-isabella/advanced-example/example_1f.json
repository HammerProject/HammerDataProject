//
// Percentage of Children (6 - 59 months)  who are Severely Undernourished
// (KENYA Average vs Turkana)
// 
// wasting_3_sd --> % severe undernutrition
// stuting_3_sd --> % severe stunting
// under_weight_3_sd --> % severe underweight
// county --> county/area of Kenya
// 
//
// in East-Aprifca
// from africaopendata.org 
//
// FUZZY QUERY!
//
//
// SQL
// SELECT updated, county1, location, under_weight_3_sd, stuting_3_sd, wasting_3_sd
// FROM nutrition_child
// WHERE county1 = "Turkana" or county1 = "Kenya Average"

{
  "select": [
  	  { "column": { "instance": "nutrition_child", "label":"updated"}},
      { "column": { "instance": "nutrition_child", "label":"county1"}},
      { "column": { "instance": "nutrition_child", "label":"location"}},
      { "column": { "instance": "nutrition_child", "label":"under_weight_3_sd"}},
      { "column": { "instance": "nutrition_child", "label":"stuting_3_sd"}},
      { "column": { "instance": "nutrition_child", "label":"wasting_3_sd"}}
  ],
  "from": [
    "nutrition_child"
  ],
  "where": [{"condition": {
      "instance1": "nutrition_child",
      "label1": "county1",
      "operator": "eq",
      "value": "Turkana",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "nutrition_child",
      "label1": "county1",
      "operator": "eq",
      "value": "Kenya Average",
      "logicalOperator": "or"
    }}
  ]
}

