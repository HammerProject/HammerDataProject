{
  "select": [
      { "column": { "instance": "fountains", "label":"property"}},
      { "column": { "instance": "fountains", "label":"site"}},
      { "column": { "instance": "fountains", "label":"drinking"}},
      { "column": { "instance": "fountains", "label":"borough"}}
  ],
  "from": [
    "fountains"
  ],
  "where": [{"condition": {
      "instance1": "fountains",
      "label1": "borough",
      "operator": "eq",
      "value": "M",
      "logicalOperator": "or"
    }}
  ]
}

