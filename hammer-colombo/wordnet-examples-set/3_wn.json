// 
// Participatory Budgeting Projects (year 2017 and category housing)
// 
//

{
  "select": [
      { "column": { "instance": "projects", "label":"title"}},
      { "column": { "instance": "projects", "label":"description"}},
      { "column": { "instance": "projects", "label":"cost"}},
      { "column": { "instance": "projects", "label":"location"}}
  ],
  "from": [
    "projects"
  ],
  "where": [{"condition": {
      "instance1": "projects",
      "label1": "year",
      "operator": "eq",
      "value": "2017",
      "logicalOperator": "and"
    }}, {"condition": {
      "instance1": "projects",
      "label1": "category",
      "operator": "eq",
      "value": "Housing",
      "logicalOperator": "or"
    }}
  ]
}

