// 
// Citywide Payroll Data (Fiscal Year)
// 
//

{
  "select": [
      { "column": { "instance": "payroll", "label":"title"}},
      { "column": { "instance": "payroll", "label":"fiscalyear"}},
      { "column": { "instance": "payroll", "label":"basesalary"}},
      { "column": { "instance": "payroll", "label":"regularhour"}}
  ],
  "from": [
    "payroll"
  ],
  "where": [{"condition": {
      "instance1": "payroll",
      "label1": "fiscalyear",
      "operator": "eq",
      "value": "2016",
      "logicalOperator": "and"
    }}
  ]
}

