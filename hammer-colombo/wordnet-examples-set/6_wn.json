//
// NYC Daily results from Watershed Water Quality Data
//

{
  "select": [
      { "column": { "instance": "results", "label":"date"}},
      { "column": { "instance": "results", "label":"fecalcoliform"}},
      { "column": { "instance": "results", "label":"averageturbidity"}},
      { "column": { "instance": "results", "label":"turbidity"}}
  ],
  "from": [
    "results"
  ],
  "where": [{"condition": {
      "instance1": "results",
      "label1": "date",
      "operator": "gt",
      "value": "2016-01-01T00:00:00",
      "logicalOperator": "and"
    }}
  ]
}


