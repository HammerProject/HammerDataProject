// 
// Water Consumption In The New York City
//

{
  "select": [
      { "column": { "instance": "water", "label":"year"}},
      { "column": { "instance": "water", "label":"population"}},
      { "column": { "instance": "water", "label":"consumption"}},
      { "column": { "instance": "water", "label":"percapita"}}
  ],
  "from": [
    "water"
  ],
  "where": [{"condition": {
      "instance1": "water",
      "label1": "year",
      "operator": "eq",
      "value": "2015",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "water",
      "label1": "year",
      "operator": "eq",
      "value": "2016",
      "logicalOperator": "or"
    }}
  ]
}

