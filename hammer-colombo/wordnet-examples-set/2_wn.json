{
  "select": [
      { "column": { "instance": "publications", "label":"publication"}},
      { "column": { "instance": "publications", "label":"category"}},
      { "column": { "instance": "publications", "label":"url"}}
  ],
  "from": [
    "publications"
  ],
  "where": [{"condition": {
      "instance1": "publications",
      "label1": "category",
      "operator": "eq",
      "value": "Health",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "publications",
      "label1": "category",
      "operator": "eq",
      "value": "Budget",
      "logicalOperator": "and"
    }}
  ]
}

