// Average price of cereals in 2015
// 
// 
//

{
  "select": [
      { "column": { "instance": "food", "label":"year"}},
      { "column": { "instance": "food", "label":"product"}},
      { "column": { "instance": "food", "label":"commodity"}},
      { "column": { "instance": "food", "label":"price"}}
  ],
  "from": [
    "food"
  ],
  "where": [{"condition": {
      "instance1": "food",
      "label1": "product",
      "operator": "eq",
      "value": "Cereals",
      "logicalOperator": "and"
    }}
  ]
}

