// Series of average price of some fruit and vegetables
// 
// 
//

{
  "select": [
      { "column": { "instance": "food", "label":"year"}},
      { "column": { "instance": "food", "label":"tradegood"}},
      { "column": { "instance": "food", "label":"value"}}
  ],
  "from": [
    "food"
  ],
  "where": [{"condition": {
      "instance1": "food",
      "label1": "tradegood",
      "operator": "eq",
      "value": "banana",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "food",
      "label1": "tradegood",
      "operator": "eq",
      "value": "apple",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "food",
      "label1": "tradegood",
      "operator": "eq",
      "value": "strawberry",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "food",
      "label1": "tradegood",
      "operator": "eq",
      "value": "tomato",
      "logicalOperator": "or"
    }}
  ]
}

