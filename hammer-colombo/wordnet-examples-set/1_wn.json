{
  "select": [
      { "column": { "instance": "monuments", "label":"name"}},
      { "column": { "instance": "monuments", "label":"park"}},
      { "column": { "instance": "monuments", "label":"location"}},
      { "column": { "instance": "monuments", "label":"dedicated"}}
  ],
  "from": [
    "monuments"
  ],
  "where": [{"condition": {
      "instance1": "monuments",
      "label1": "installation",
      "operator": "gt",
      "value": "2010/01/01",
      "logicalOperator": "or"
    }}
  ]
}

