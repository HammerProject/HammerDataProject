//
// Percentage of Children (6 - 59 months)  who are Severely Undernourished
// (KENYA Average vs Turkana)
// 
// wasting_3_sd --> % severe undernutrition
// stuting_3_sd --> % severe stunting
// under_weight_3_sd --> % severe underweight
// county --> county/area of Kenya
// 
//
// in East-Aprifca
// from africaopendata.org 
//
// 
//
// ORIGINAL QUERY:
// SQL
// SELECT updated, county1, location, under_weight_3_sd, stuting_3_sd, wasting_3_sd
// FROM nutrition_child
// WHERE county1 = "Turkana" or county1 = "Kenya Average"
//
// FUZZY QUERY!

{
  "select": [
  	  { "column": { "instance": "nutrition", "label":"updated"}},
      { "column": { "instance": "nutrition", "label":"county"}},
      { "column": { "instance": "nutrition", "label":"location"}},
      { "column": { "instance": "nutrition", "label":"under_weight"}},
      { "column": { "instance": "nutrition", "label":"stuting"}},
      { "column": { "instance": "nutrition", "label":"wasting"}}
  ],
  "from": [
    "nutrition"
  ],
  "where": [{"condition": {
      "instance1": "nutrition",
      "label1": "county",
      "operator": "eq",
      "value": "Turkana",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "nutrition",
      "label1": "county",
      "operator": "eq",
      "value": "Kenya Average",
      "logicalOperator": "or"
    }}
  ]
}

