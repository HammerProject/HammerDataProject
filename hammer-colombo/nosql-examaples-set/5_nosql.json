//
// Return all job-offers in DE for SAP Finance Experts
// 
//

{
  "select": [
      { "column": { "instance": "jobs", "label":"title"}},
      { "column": { "instance": "jobs", "label":"description"}},
      { "column": { "instance": "jobs", "label":"place"}},
      { "column": { "instance": "jobs", "label":"country"}}
  ],
  "from": [
    "jobs"
  ],
  "where": [{"condition": {
      "instance1": "jobs",
      "label1": "title",
      "operator": "eq",
      "value": "SAP Finance Expert",
      "logicalOperator": "and"
    }}, {"condition": {
      "instance1": "jobs",
      "label1": "country",
      "operator": "eq",
      "value": "DE",
      "logicalOperator": "and"
    }}
  ]
}

