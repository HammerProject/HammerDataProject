// Political Party Fund Allocations 2015
//

{
  "select": [
      { "column": { "instance": "party", "label":"party"}},
      { "column": { "instance": "party", "label":"proportional"}},
      { "column": { "instance": "party", "label":"equitable"}}
  ],
  "from": [
    "party"
  ],
  "where": [{"condition": {
      "instance1": "party",
      "label1": "year",
      "operator": "eq",
      "value": "2015",
      "logicalOperator": "or"
    }}
  ]
}

