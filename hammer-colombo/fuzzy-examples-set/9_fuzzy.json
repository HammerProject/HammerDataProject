// 
// Candidates election from 2010
//

{
  "select": [
      { "column": { "instance": "candidates", "label":"party"}},
      { "column": { "instance": "candidates", "label":"surname"}},
      { "column": { "instance": "candidates", "label":"name"}}
  ],
  "from": [
    "candidates"
  ],
  "where": [{"condition": {
      "instance1": "candidates",
      "label1": "year",
      "operator": "ge",
      "value": "2010",
      "logicalOperator": "or"
    }}
  ]
}

