// Organization for Women Emergency (24 hour crisis intervention)
//

{
  "select": [
      { "column": { "instance": "womenemergency", "label":"services"}},
      { "column": { "instance": "womenemergency", "label":"suburb"}},
      { "column": { "instance": "womenemergency", "label":"crisiscounsellingline"}},
      { "column": { "instance": "womenemergency", "label":"contactperson"}},
 	  { "column": { "instance": "womenemergency", "label":"contactnumber"}},
 	  { "column": { "instance": "womenemergency", "label":"sms"}},
 	  { "column": { "instance": "womenemergency", "label":"email"}}
  ],
  "from": [
    "womenemergency"
  ],
  "where": [{"condition": {
      "instance1": "womenemergency",
      "label1": "services",
      "operator": "eq",
      "value": "24 hour intervention",
      "logicalOperator": "or"
    }}
  ]
}

