// 
// Water Consumption by city, year and description (year = 2013)
//

{
  "select": [
      { "column": { "instance": "waterconsumption", "label":"city"}},
  	  { "column": { "instance": "waterconsumption", "label":"date"}},
 	  { "column": { "instance": "waterconsumption", "label":"description"}},
      { "column": { "instance": "waterconsumption", "label":"consumption_per_capita"}}
  ],
  "from": [
    "waterconsumption"
  ],
  "where": [{"condition": {
      "instance1": "waterconsumption",
      "label1": "date",
      "operator": "eq",
      "value": "2013-12-31t00:00:00",
      "logicalOperator": "or"
    }}
  ]
}

