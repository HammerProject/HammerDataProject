{
  "select": [
      { "column": { "instance": "collisions", "label":"date"}},
  	  { "column": { "instance": "collisions", "label":"latitude"}},
 	  { "column": { "instance": "collisions", "label":"longitude"}},
 	  { "column": { "instance": "collisions", "label":"vehicle_type_code"}}
  ],
  "from": [
    "collisions"
  ],
  "where": [{"condition": {
      "instance1": "collisions",
      "label1": "date",
      "operator": "gt",
      "value": "2016/01/01",
      "logicalOperator": "and"
    }}
  ]
}

