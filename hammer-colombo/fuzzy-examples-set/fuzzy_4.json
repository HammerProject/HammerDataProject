{
  "select": [
  	  { "column": { "instance": "twitter_statistics", "label":"date"}},
  	  { "column": { "instance": "twitter_statistics", "label":"agency"}},
  	  { "column": { "instance": "twitter_statistics", "label":"url"}},
      { "column": { "instance": "twitter_statistics", "label":"statistics"}}
  ],
  "from": [
    "twitter_statistics"
  ],
  "where": [{"condition": {
      "instance1": "twitter_statistics",
      "label1": "agency",
      "operator": "eq",
      "value": "311",
      "logicalOperator": "and"
    }}
  ]
}

