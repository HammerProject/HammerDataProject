{
  "select": [
      { "column": { "instance": "buildings", "label":"permits"}},
      { "column": { "instance": "buildings", "label":"data"}},
      { "column": { "instance": "buildings", "label":"city"}}
  ],
  "from": [
    "buildings"
  ],
  "where": [{"condition": {
      "instance1": "buildings",
      "label1": "recordTypeGroup",
      "operator": "eq",
      "value": "Building",
      "logicalOperator": "or"
    }}, {"condition": {
      "instance1": "buildings",
      "label1": "LicenseType",
      "operator": "eq",
      "value": "Rental",
      "logicalOperator": "or"
    }}
  ]
}

